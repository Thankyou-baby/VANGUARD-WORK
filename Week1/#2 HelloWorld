#include <iostream>
using std::cout;
using std::cin;
int main()
{
	int input;
	for(;;)
	{
		cout << "please input negative one , zero , or positive one: ";
		cin >> input;
		if(input == -1)
		{
			break;
		}
		else if(input == 0 )
		{
			cout << "helloworld" << "\n";
		}
		else if(input == 1)
		{
			cout << "HELLOWORLD" << "\n";
		}
	}
	return 0;
}
